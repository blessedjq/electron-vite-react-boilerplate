{
  "name": "electron-react-boilerplate",
  "version": "1.0.0",
  "private": true,
  "main": "electron/main.cjs",
  "type": "commonjs",
  "scripts": {
    "vite": "npx vite",
    "electron": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 npx electron ./electron/main.cjs",
    "dev": "node ./electron/dev.js",
  "build": "vite build",
  "electron-build": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 npx electron ./electron/main.cjs",
    "package": "npm run build && electron-builder --publish never"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.0",
    "electron": "^25.0.0",
    "electron-builder": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "vite": "^4.0.0",
    "wait-on": "^7.0.0"
  },
  "build": {
    "appId": "com.boilerplate.app",
    "productName": "ElectronReactBoilerplate",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**",
      "electron/**",
      "node_modules/**",
      "package.json"
    ],
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  }
}
